{
  "name": "@concord-consortium/glossary-plugin",
  "version": "4.1.0",
  "description": "Glossary LARA plugin",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --devtool eval --hot",
    "start:secure:no-certs": "webpack-dev-server --https --hot",
    "build": "npm-run-all clean build:webpack",
    "build:webpack": "webpack --mode production",
    "clean": "rimraf dist",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "strings:pull:usage": "echo 'Usage: npm run strings:pull -- -a <poeditor_api_key>'",
    "strings:pull": "./poeditor-scripts/strings-pull-project.sh",
    "strings:push:usage": "echo 'Usage: npm run strings:push -- -a <poeditor_api_key>'",
    "strings:push": "./poeditor-scripts/strings-push-project.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/glossary-plugin.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/glossary-plugin/issues"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testURL": "https://glossary-test.unexisting.url.com",
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  },
  "homepage": "https://github.com/concord-consortium/glossary-plugin#readme",
  "devDependencies": {
    "@types/clone": "^0.1.30",
    "@types/dom-mediacapture-record": "^1.0.2",
    "@types/enzyme": "^3.10.3",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^27.4.1",
    "@types/jsdom": "^11.12.0",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/pluralize": "0.0.29",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.8.3",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "autoprefixer": "^9.5.1",
    "copy-webpack-plugin": "^9.1.0",
    "css-loader": "^6.8.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.1.2",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^1.7.5",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^13.3.2",
    "style-loader": "^0.22.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "util": "^0.12.4",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "dependencies": {
    "@concord-consortium/lara-plugin-api": "3.3.1",
    "@concord-consortium/token-service": "^1.1.0",
    "ajv": "^6.12.3",
    "ajv-keywords": "^3.4.0",
    "aws-sdk": "^2.814.0",
    "client-oauth2": "^4.2.5",
    "clone": "^2.1.2",
    "dexie": "^3.0.3",
    "file-saver": "^2.0.2",
    "firebase": "^10.6.0",
    "html-webpack-plugin": "^5.5.3",
    "jsonwebtoken": "^8.5.1",
    "pluralize": "^8.0.0",
    "react-dropzone": "^6.2.4",
    "react-json-editor-ajrm": "^2.5.9",
    "react-modal": "^3.10.1",
    "uuid": "^3.3.2"
  }
}
